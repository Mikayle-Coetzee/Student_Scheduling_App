<UserControl x:Class="_10023767_P2.Controls.BindablePasswordBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:_10023767_P2.Controls"
             mc:Ignorable="d" 
                         d:DesignHeight="30" d:DesignWidth="270">
    <!--#region Password Box -->
    <PasswordBox x:Name="txtPassword" 
             HorizontalAlignment="Center" 
             VerticalAlignment="Center" 
             ToolTip="Enter your registered password"
             Background="{x:Null}"
             VerticalContentAlignment="Center"
             BorderBrush="{DynamicResource borderBrushDark}" Height="28" Width="270"/>

    <!--#endregion-->
</UserControl>
